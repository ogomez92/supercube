{
  "name": "Supercube",
  "version": "2.0.5",
  "description": "Supercube game",
  "main": "./client/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "parcel watch ./client/js/main.js -d ./client --target=electron",
    "build": "parcel build ./client/js/main.js -d ./client --target=electron",
    "bundle": "parcel build ./client/js/main.js -d ./client --no-source-maps",
    "start": "electron ./client",
    "run": "npm run build && npm run start",
    "server": "http-server ./client"
  },
  "repository": {
    "type": "git",
    "url": "http://oriolgomez.com:9900/ogomez92/beatjs"
  },
  "author": "Oriol Gomez",
  "license": "GPL",
  "dependencies": {
    "agk-ui": "0.0.2",
    "babel-polyfill": "^6.26.0",
    "copy-to-clipboard": "^3.2.0",
    "hammerjs": "^2.0.8",
    "howler": "^2.1.2",
    "jquery": "^3.4.1",
    "number-words": "^1.1.0",
    "parcel-bundler": "^1.12.3",
    "s": "^0.1.1",
    "sono": "^2.1.6",
    "speak-tts": "^2.0.8",
    "string-digits": "0.0.6",
    "unique-random": "^2.1.0"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0"
  },
  "build":{
    "compression": "maximum",
    "win": {
      "target": [
        {
          "target": "zip",
          "arch": [
            "ia32"
          ]
        }
      ]
}
  }
}
